{
  "name": "Jira Worklog",
  "description": "Jira Helper",
  "version": "4.3",
  "manifest_version": 3,
  "icons": {
    "64": "jira.png"
  },
  "homepage_url": "https://github.com/panumatMk/jira-extension-vue",
  "action": {
    "default_popup": "index.html",
    "default_icon": "jira.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}
